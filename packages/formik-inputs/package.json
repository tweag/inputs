{
  "name": "formik-inputs",
  "license": "MIT",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/formik-inputs.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "parcel example/index.html",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "format": "prettier --write '{src,example}/**/*.{ts,tsx}'",
    "format:check": "prettier --check '{src,example}/**/*.{ts,tsx}'",
    "typecheck": "tsc -p . --noEmit",
    "prepublishOnly": "tsdx test && tsdx build"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "peerDependencies": {
    "formik": "^1.5.8",
    "react": ">=16"
  },
  "dependencies": {
    "react-baseline-inputs": "^0.5.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "formik": "^2.0.1-rc.12",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.7.2",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  }
}
